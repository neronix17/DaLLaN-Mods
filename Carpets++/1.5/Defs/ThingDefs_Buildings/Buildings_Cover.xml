		<label>SimpleCarpet 1</label>
		<description>Simple carpet without complicated patterns with built-in electro wire.\n Texture: 1.\n Basic beauty: 3</description>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<graphicData>
			<texPath>Carpets/CapretDan_1</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>Transmitter</linkType>
			<linkFlags>
				<li>Custom7</li>
			</linkFlags>
		</graphicData>
		<uiIconPath>CarpetIcons/CapretDan_1</uiIconPath>
