<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<RecipeDef>
		<defName>MakeMultiCloth</defName>
		<label>Make multicloth x5</label>
		<description>Make multicloth from skin/cloth, using special gas.</description>
		<jobString>Making Multicloth</jobString>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>Tailor</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<workAmount>500</workAmount>
		<recipeUsers>
			<li>MechaWorkbench</li>
		</recipeUsers>
		<targetCountAdjustment>5</targetCountAdjustment>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Leathers</li>
					</categories>
					<thingDefs>
						<li>Cloth</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Gasnenkj</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Leathers</li>
			</categories>
			<thingDefs>
				<li>Cloth</li>
				<li>Gasnenkj</li>
			</thingDefs>
			<disallowedThingDefs>
				<li>Leather_Thrumbo</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<products>
			<MultiCloth>5</MultiCloth>
		</products>
		<workSkill>Crafting</workSkill>
	</RecipeDef>

